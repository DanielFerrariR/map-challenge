    "react-hot-loader": "4.12.21",
    "react-redux": "7.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "typeface-roboto": "1.1.13",
    "yarn": "1.22.4"
  },
