{
  "name": "map-challenge",
  "version": "0.3.0",
  "description": "Map Challenge",
  "homepage": "https://reactjs.org",
  "license": "UNLICENSED",
  "private": true,
  "author": {
    "name": "Daniel",
    "email": "danielferrari@live.com",
    "url": "https://github.com/DanielFerrariR"
  },
  "main": "src",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielFerrariR/map-challenge.git"
  },
  "scripts": {
    "commit": "git add . && git-cz",
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "start": "serve --no-clipboard --single --listen 3000 dist",
    "build": "webpack --config webpack.config.prod.js",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "pretest:coverage": "rimraf jest_coverage",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.md,.mdx .",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "precy:run": "rimraf .nyc_output && rimraf cypress_coverage",
    "cy:run": "cypress run --browser=chrome --headless",
    "precy:open": "rimraf .nyc_output && rimraf cypress_coverage",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "cross-env IS_CYPRESS=true yarn build",
    "test:e2e:run": "cross-env DEBUG=start-server-and-test start-server-and-test start http://localhost:3000 cy:run",
    "dev:cypress": "cross-env IS_CYPRESS=true webpack-dev-server --config webpack.config.dev.js",
    "test:e2e:dev": "cross-env DEBUG=start-server-and-test start-server-and-test dev:cypress http://localhost:3000 cy:open",
    "tsc": "tsc",
    "tsc:cypress": "tsc -p spec/cypress/tsconfig.json",
    "check-types": "yarn tsc && yarn tsc:cypress",
    "prettier": "prettier \"**/*.+(js|jsx|json|ts|tsx|md|mdx|html|eslintrc|prettierrc)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint test:coverage test:e2e:run",
    "postvalidate": "yarn report:combined",
    "precopy:reports": "rimraf reports && mkdir reports",
    "copy:reports": "cp cypress_coverage/coverage-final.json reports/from-cypress.json && cp jest_coverage/coverage-final.json reports/from-jest.json",
    "precombine:reports": "yarn copy:reports && rimraf .nyc_output && mkdir .nyc_output",
    "combine:reports": "nyc merge reports && mv coverage.json .nyc_output/out.json",
    "prereport:combined": "yarn combine:reports && rimraf coverage",
    "report:combined": "nyc report --reporter lcov --report-dir coverage",
    "setup": "yarn && npx cypress install && yarn validate",
    "preanalyze": "cross-env IS_ANALYZE=true yarn build",
    "analyze": "source-map-explorer 'dist/*.js'",
    "storybook": "start-storybook -p 3001 -c storybook --docs"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@material-ui/system": "4.9.14",
    "@react-google-maps/api": "2.2.0",
    "axios": "0.19.2",
    "nanoid": "3.1.12",
